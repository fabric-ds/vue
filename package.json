{
  "name": "@fabric-ds/vue",
  "version": "0.12.0",
  "description": "Fabric components for Vue 3",
  "type": "module",
  "exports": "./dist/fabric-vue.js",
  "imports": {
    "#util": "./components/util/index.js",
    "#components": "./index.js",
    "#generics": "./components/generic/index.js"
  },
  "scripts": {
    "dev": "cd example && vite",
    "build": "rm -rf dist && npm run build:npm && npm run build:eik",
    "build:npm": "rollup -c",
    "build:eik": "eik=true rollup -c",
    "publish:eik": "npx @eik/cli publish && npx @eik/cli pkg-alias",
    "site": "npm run site:build && npm run site:cloud",
    "site:build": "cd example && vite build",
    "site:cloud": "cd example/dist && cp index.html 200.html && cp index.html 404.html && cp ../CNAME .",
    "preversion": "npx @eik/cli login && npm run build",
    "version": "npm publish",
    "postversion": "npm run publish:eik && git push --follow-tags"
  },
  "keywords": [],
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@eik/rollup-plugin": "^4.0.17",
    "@fabric-ds/component-classes": "^0.0.38",
    "@fabric-ds/core": "^0.0.8",
    "@finn-no/dom-focus-lock-fixed": "^1.0.6",
    "@floating-ui/dom": "^0.4.4",
    "create-v-model": "^2.1.2",
    "element-collapse": "^1.0.1",
    "femtobounce": "^1.0.0",
    "scroll-doctor": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "cleave-lite": "^1.0.0",
    "drnm": "^0.9.0",
    "rollup": "^2.70.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^2.9.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "eik": {
    "server": "https://assets.finn.no",
    "files": "./dist/eik",
    "import-map": "https://assets.finn.no/map/finn/v2"
  }
}
