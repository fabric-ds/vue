{
  "name": "@fabric-ds/vue",
  "version": "0.9.3",
  "description": "Fabric components for Vue 3",
  "type": "module",
  "exports": "./dist/fabric-vue.js",
  "imports": {
    "#util": "./components/util/index.js",
    "#components": "./index.js"
  },
  "scripts": {
    "dev": "cd example && vite",
    "build": "rollup -c",
    "build:eik": "eik=true npm run build",
    "site": "npm run site:build && npm run site:cloud",
    "site:build": "cd example && vite build",
    "site:cloud": "cd example/dist && cp index.html 200.html && cp index.html 404.html && cp ../CNAME .",
    "preversion": "npm run build",
    "version": "npm publish",
    "postversion": "git push --follow-tags"
  },
  "keywords": [],
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fabric-ds/component-classes": "^0.0.31",
    "@finn-no/dom-focus-lock-fixed": "^1.0.6",
    "create-v-model": "^2.1.2",
    "element-collapse": "^0.9.1",
    "femtobounce": "^1.0.0",
    "scroll-doctor": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "cleave-lite": "^1.0.0",
    "drnm": "^0.9.0",
    "rollup": "^2.58.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^2.6.7",
    "vue": "^3.2.20",
    "vue-router": "^4.0.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ]
}
