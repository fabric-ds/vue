{
  "name": "@fabric-ds/vue",
  "version": "0.11.2-next.1",
  "description": "Fabric components for Vue 3",
  "type": "module",
  "exports": "./dist/fabric-vue.js",
  "imports": {
    "#util": "./components/util/index.js",
    "#components": "./index.js",
    "#generics": "./components/generic/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-ds/vue.git"
  },
  "scripts": {
    "dev": "cd example && vite",
    "build": "rm -rf dist && npm run build:npm && npm run build:eik",
    "build:npm": "rollup -c",
    "build:eik": "eik=true rollup -c",
    "site": "npm run site:build && npm run site:cloud",
    "site:build": "cd example && vite build",
    "site:cloud": "cd example/dist && cp index.html 200.html && cp index.html 404.html && cp ../CNAME .",
    "semantic-release": "semantic-release",
    "eik:login": "npx @eik/cli login",
    "eik:publish": "npx @eik/cli publish"
  },
  "keywords": [],
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@eik/rollup-plugin": "^4.0.14",
    "@fabric-ds/component-classes": "^0.0.36",
    "@finn-no/dom-focus-lock-fixed": "^1.0.6",
    "@floating-ui/dom": "^0.2.0",
    "create-v-model": "^2.1.2",
    "element-collapse": "^1.0.1",
    "femtobounce": "^1.0.0",
    "scroll-doctor": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.31",
    "cleave-lite": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "drnm": "^0.9.0",
    "rollup": "^2.68.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "semantic-release": "^19.0.2",
    "semantic-release-slack-bot": "^3.5.2",
    "vite": "^2.8.4",
    "vue": "^3.2.31",
    "vue-router": "^4.0.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "eik": {
    "server": "https://assets.finn.no",
    "files": "./dist/eik",
    "import-map": "https://assets.finn.no/map/finn/v2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
