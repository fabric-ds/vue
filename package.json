{
  "name": "@fabric-ds/vue",
  "version": "0.9.0",
  "description": "Fabric components for Vue 3",
  "type": "module",
  "exports": "./dist/fabric-vue.js",
  "imports": {
    "#util": "./components/util/index.js",
    "#components": "./index.js"
  },
  "scripts": {
    "prepublish": "yarn build",
    "dev": "yarn lerna run --parallel dev",
    "build": "yarn lerna run build --parallel",
    "site": "yarn build && yarn lerna run site --parallel && yarn run site:cloud",
    "site:cloud": "cd example/dist && cp index.html 200.html && cp index.html 404.html"
  },
  "keywords": [],
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fabric-ds/component-classes": "^0.0.31",
    "create-v-model": "^2.1.2",
    "element-collapse": "^0.9.1",
    "@finn-no/dom-focus-lock-fixed": "^1.0.6",
    "scroll-doctor": "^0.9.1",
    "femtobounce": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vue/compiler-sfc": "^3.2.11",
    "rollup": "^2.56.3",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
